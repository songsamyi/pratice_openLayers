{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ES5",
    "module": "commonJS",
    "strict": true,
    "allowJs": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": ".",
    "noEmit": true,
    "experimentalDecorators": true
  }
}
